
server {
	server_name default
	listen 127.0.0.1:8080
	body_size 10240
	method GET POST DELETE
	error_page 404 www/404/html
	
	location test/path {
		status 200
		directory_list on
		redirection jinho1
		method GET POST
		file_path www/index.html www/default.html
	}

	location .py {
		status 404
		directory_list on
		cgi_pass cgi/cgi.py
	}
}

server {
	server_name default2
	listen 127.0.0.1:8081
	body_size 20480
	method GET POST
	error_page 500 www/500/html
}