
server {
	listen 8080
	body_size 10240
	root temp/
	file_path temp/index.html
	upload_path temp/

	server_name config.com
	timeout 10
	directory_list on

	method GET POST PUT DELETE
	error_page 404 temp/index.html

	location test/path {
		status_code 200
		method GET POST
		file_path temp/index.html
		redirection 300 temp/
	}
	
	location .py {
		status_code 200
		cgi_pass cgi/cgi.py
	}
}

server {
	listen 8081
	body_size 20480
	root temp/
	upload_path temp/
	method GET POST
	error_page 500 temp/index.html

	server_name default
	timeout 30

	location .py {
		status_code 500
		cgi_pass cgi/cgi.py
	}
}
