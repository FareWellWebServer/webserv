server {
	port 8080
	host 127.0.0.1
    redirection default1
    server_name default
    method GET POST DELETE
    error_page www/404/html
	location test/path {
		status 200
		autoindex on
		redirection default2
		method GET POST
		file_path www/index.html www/default.html
	}
	location .py {
		status 404
		cgi_pass cgi/cgi.py
	}
}