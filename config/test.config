server {
	listen 127.0.0.1:8080
	error_page 404 html/index.html
	body_size 10240
	method GET POST DELETE

	server_name default

	location test/path {
		status_code 200
		directory_list on
		redirection jinho
		method GET POST
		file_path html/index.html
	}

	location .py {
		status_code 404
		cgi_pass cgi/cgi.py
	}
}

server {
	listen 127.0.0.1:8081
	error_page 500 html/index.html
	body_size 20480
	method GET POST

	server_name default

	location .py {
		status_code 500
		cgi_pass cgi/cgi.py
	}
}